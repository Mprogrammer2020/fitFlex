<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item

        android:bottom="-5px"
        android:left="3px"
        android:right="-5px"
        android:top="-5px">
        <shape>
            <solid android:color="@android:color/transparent" />

            <stroke
                android:width="1dp"
                android:color="@color/grey"
                android:dashWidth="7dp"
                android:dashGap="5dp"/>
        </shape>
    </item>
</layer-list>
